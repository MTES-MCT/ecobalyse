{
  "foo": "dist/foo/index.js",
  "bar": "dist/browser/index.js",
  "engines": {
    "node": ">= 8.0.0"
  },
  "targets": {
    "foo": {
      "engines": {
        "browsers": ["last 1 version"]
      },
      "sourceMap": {
        "inlineSources": true
      }
    },
    "bar": {
      "engines": {
        "browsers": ["last 1 version"]
      },
      "publicUrl": "/assets"
    }
  }
}
