{
    "postprocess": [
        "elm-watch-node",
        "postprocess.mjs"
    ],
    "targets": {
        "frontend": {
            "inputs": [
                "src/Main.elm"
            ],
            "output": "public/build/main.js"
        },
        "server": {
            "inputs": [
                "src/Server.elm"
            ],
            "output": "server-app.js"
        }
    }
}
