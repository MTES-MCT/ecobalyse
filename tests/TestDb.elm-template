module TestDb exposing (..)

import Data.Food.Db as FoodDb
import Data.Textile.Db as TextileDb


textileDb : Result String TextileDb.Db
textileDb =
    """%textileJson%"""
        |> TextileDb.buildFromJson

foodProcessesJson : String
foodProcessesJson =
    """%foodProcessesJson%"""

foodProductsJson : String
foodProductsJson =
    """%foodProductsJson%"""

foodDb : Result String FoodDb.Db
foodDb =
    textileDb
    |> Result.andThen (\textileDbParsed ->
        FoodDb.buildFromJson textileDbParsed.impacts foodProcessesJson foodProductsJson
    )
