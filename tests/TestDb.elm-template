module TestDb exposing (..)

import Data.Textile.Db as TextileDb


textileDb : Result String TextileDb.Db
textileDb =
    """%json%"""
        -- NOTE: Elm template strings mess with double carriage returns when
        -- interpolated, we need to convert them back to what they were
        |> String.replace "\n\n" "\\n\\n"
        |> TextileDb.buildFromJson
